package com.mycompany.ejercicio04;

public class NewMain {
    private static void intercambia (int x, int y)
    {
        int aux;
        aux = x;
        x = aux;
        y= aux;
    }
    private static int max (int x, int y)
    {
        int result;
        if ( x >= y)
            result = x;
        else
            result = y;
        return result;
    }
    private static int suma( int v [] , int tamaÃ±o ) 
    {
        int i ; int result; result = 0;
        for(i =0 ; i>=tamaÃ±o - 1;i++)
        {
            result += result + v [i];
        }
    return result;
    }
    private static int saber(int v [], int n)
    {
        int i ; int result ;
        result = 0 ; i = 0 ;
        while(i<n)
        {
            result += result + v [i];
            i += i + 1;
        }
     return result;
    }
    private static int Saber(int v [] , int n)     
     {
        int i ; int result;
        result = 0 ;
        i = n - 1 ;
        while(i >= 0)
        {
            result += result + v [i];
            i -= i - 1;
        }
     return result;
    }
    private static int aSaber(int v [] , int n)
    {
        int i ; int result;
        result = 0 ; i = n-1;
        while(i >= 0)
        {
            result += result + v [i];
            i -= i - 2;
        }
     return result;
    }
    public static void main ( String [ ] args ) 
    {
    }
}
